{
  "name": "@fruits-chain/graphql-kit-helpers",
  "version": "0.0.0",
  "description": "Collection of helper functions for graphql-kit",
  "main": "dist/index.cjs",
  "type": "module",
  "module": "dist/index.mjs",
  "types": "dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules yarn jest",
    "clean": "rimraf dist temp",
    "lint": "eslint",
    "build-bundle": "rollup -c",
    "build-dts": "tsc --emitDeclarationOnly --declaration --outDir temp",
    "build-types": "yarn build-dts && api-extractor run && rimraf temp",
    "build": "run-s clean lint test build-bundle build-types"
  },
  "keywords": [
    "graphql-kit",
    "helpers",
    "mock",
    "doc"
  ],
  "author": "changhuali <changhua.li.cq@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/changhuali/graphql-kit",
    "directory": "packages/helpers"
  },
  "bugs": {
    "url": "https://github.com/changhuali/graphql-kit/issues"
  },
  "homepage": "https://github.com/changhuali/graphql-kit/tree/main/packages/helpers#readme",
  "peerDependencies": {
    "graphql": "^16.2.0"
  },
  "devDependencies": {
    "graphql": "^16.2.0"
  }
}
