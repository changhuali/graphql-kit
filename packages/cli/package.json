{
  "name": "@graphql-kit/cli",
  "version": "0.0.1",
  "description": "A cli for graphql-kit",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/types.d.ts",
  "files": [
    "bin",
    "dist"
  ],
  "bin": {
    "gk": "bin/index.js"
  },
  "scripts": {
    "dev": "npm run clean && rollup -c -w",
    "cli": "node ./dist/index.cjs",
    "clean": "rimraf dist temp",
    "lint": "eslint",
    "build-bundle": "rollup -c",
    "build": "run-s clean lint build-bundle"
  },
  "keywords": [
    "graphql-kit",
    "cli"
  ],
  "author": "changhuali <changhua.li.cq@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@graphql-kit/server": "0.0.1",
    "chalk": "3.0.0",
    "commander": "^8.3.0",
    "deepmerge": "^4.2.2",
    "inquirer": "^8.2.0",
    "stringify-object": "3.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "^8.1.3",
    "@types/stringify-object": "^4.0.1"
  }
}
