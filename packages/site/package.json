{
  "name": "@graphql-kit/site",
  "description": "A doc site for graphql-kit",
  "version": "0.0.1",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/types.d.ts",
  "scripts": {
    "dev": "next dev",
    "clean": "rimraf dist temp",
    "lint": "eslint",
    "build-bundle": "next build && rollup -c",
    "build-dts": "tsc src/server.ts --emitDeclarationOnly --declaration --skipLibCheck --noEmit false --outDir temp",
    "build-types": "yarn build-dts && api-extractor run && rimraf temp",
    "build": "run-s clean lint build-bundle build-types",
    "start": "next start"
  },
  "files": [
    "dist",
    ".next"
  ],
  "keywords": [
    "graphql-kit",
    "server",
    "mock",
    "doc"
  ],
  "author": "changhuali <changhua.li.cq@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/changhuali/graphql-kit",
    "directory": "packages/site"
  },
  "bugs": {
    "url": "https://github.com/changhuali/graphql-kit/issues"
  },
  "homepage": "https://github.com/changhuali/graphql-kit/tree/main/packages/site#readme",
  "dependencies": {
    "next": "12.0.8"
  },
  "devDependencies": {
    "@fruits-chain/hooks-laba": "^0.0.5",
    "@types/react": "17.0.38",
    "ace-builds": "^1.4.13",
    "antd": "^4.18.5",
    "classnames": "^2.3.1",
    "clipboard": "^2.0.8",
    "eslint-config-next": "12.0.8",
    "react": "17.0.2",
    "react-ace": "^9.5.0",
    "react-dom": "17.0.2",
    "zustand": "^3.6.9"
  }
}
