{
  "name": "@fruits-chain/graphql-kit-server",
  "version": "0.0.0",
  "description": "A server which can serve a mock server and API docs for your graphql service",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/types.d.ts",
  "module": "dist/index.mjs",
  "scripts": {
    "clean": "rimraf dist temp",
    "lint": "eslint",
    "build-bundle": "rollup -c",
    "build-dts": "tsc --emitDeclarationOnly --declaration --outDir temp",
    "build-types": "yarn build-dts && api-extractor run && rimraf temp",
    "build": "run-s clean lint build-bundle build-types"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "graphql-kit",
    "server",
    "mock",
    "doc"
  ],
  "author": "changhuali <changhua.li.cq@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/changhuali/graphql-kit",
    "directory": "packages/server"
  },
  "bugs": {
    "url": "https://github.com/changhuali/graphql-kit/issues"
  },
  "homepage": "https://github.com/changhuali/graphql-kit/tree/main/packages/server#readme",
  "dependencies": {
    "@fruits-chain/graphql-kit-helpers": "0.0.0",
    "@fruits-chain/graphql-kit-site": "0.0.0",
    "@graphql-tools/load": "^7.5.0",
    "@graphql-tools/mock": "^8.5.0",
    "@graphql-tools/url-loader": "^7.7.0",
    "body-parser": "^1.19.1",
    "dayjs": "^1.10.7",
    "express": "^4.17.2",
    "express-graphql": "^0.12.0",
    "graphql": "^16.2.0",
    "graphql-playground-middleware-express": "1.7.22",
    "graphql-tools": "^8.2.0",
    "http-proxy-middleware": "^2.0.1"
  },
  "devDependencies": {}
}
