{
  "name": "@graphql-kit/server",
  "version": "0.0.1",
  "description": "A server with abilities such as Mock and Doc etc",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/types.d.ts",
  "module": "dist/index.mjs",
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules yarn jest",
    "clean": "rimraf dist temp",
    "lint": "eslint",
    "build-bundle": "rollup -c",
    "build-dts": "tsc --emitDeclarationOnly --declaration --outDir temp",
    "build-types": "yarn build-dts && api-extractor run && rimraf temp",
    "build": "run-s clean lint build-bundle build-types"
  },
  "files": [
    "dist",
    "doc-site"
  ],
  "author": "changhuali <changhua.li.cq@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@graphql-tools/load": "^7.5.0",
    "@graphql-tools/mock": "^8.5.0",
    "@graphql-tools/url-loader": "^7.7.0",
    "body-parser": "^1.19.1",
    "dayjs": "^1.10.7",
    "express": "^4.17.2",
    "express-graphql": "^0.12.0",
    "graphql": "^16.2.0",
    "graphql-playground-html": "^1.6.30",
    "graphql-tools": "^8.2.0",
    "http-proxy-middleware": "^2.0.1",
    "@graphql-kit/helpers": "0.0.1"
  },
  "devDependencies": {}
}
